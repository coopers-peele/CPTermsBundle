{
    "name": "cp/terms-bundle",
    "description": "Symfony bundle to manage your app's terms of service",
    "keywords": ["terms of service", "terms and conditions", "terms", "tos", "symfony", "bundle"],
    "homepage": "https://github.com/coopers-peele/CPTermsBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Coopers Peele Limited",
            "email": "info@coopers-peele.com",
            "homepage": "http://www.coopers-peele.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle": "~2.0",
        "sensio/framework-extra-bundle": "~3.0",
        "propel/propel-bundle": "~1.4",
        "willdurand/propel-typehintable-behavior": "1.0.*",
        "cebe/markdown": "*",
        "cogpowered/finediff": "0.3.*",
        "dzango/twig-truncate-extension": "~1.0"
    },
    "autoload": {
        "psr-4": { "CP\\Bundle\\TermsBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
